{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle": ["error", { "allow": ["__dirname"] }],
        "linebreak-style": ["error", "windows"],
        "quotes": [2, "double"]
    }
}
